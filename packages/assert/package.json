{
	"name": "@moneytor/assert",
	"description": "Tiny assert utility",
	"sideEffects": false,
	"type": "module",
	"imports": {
		"#src/*": "./dist/src/*"
	},
	"exports": {
		".": {
			"default": "./dist/src/mod.js",
			"types": "./dist/src/mod.d.ts"
		}
	},
	"scripts": {
		"build": "tsc --noCheck",
		"dev": "tsc --noCheck",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"typecheck": "tsc --build --noEmit"
	},
	"devDependencies": {
		"@moneytor/eslint-config": "workspace:*",
		"@moneytor/typescript-config": "workspace:*",
		"eslint": "catalog:",
		"typescript": "catalog:"
	}
}
